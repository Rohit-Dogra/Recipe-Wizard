{
  "expo": {
    "name": "Recipe Wizard",
    "description": "Recipe generating application :)",
    "slug": "RW",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.3.0",
    "orientation": "portrait",
	"androidStatusBar": {
      "backgroundColor": "#000000"
    },
    "icon": "logo1.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
	"android": {
	  "versionCode": 3, 
      "package": "com.recipewizard",
	  "permissions": []
    }
  }
}



